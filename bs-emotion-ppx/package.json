{
  "name": "@ahrefs/bs-emotion-ppx",
  "version": "0.0.7",
  "description": "Adds labels to CSS classes generated by bs-emotion",
  "author": "Alex Fedoseev <alex.fedoseev@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahrefs/bs-emotion.git"
  },
  "main": "./src/EmotionPpx.re",
  "scripts": {
    "build": "bsb -make-world -backend native",
    "test": "bsb -make-world -backend js",
